<launch>
	<!-- launches all the gripper related stuff -->
	<!-- maintainer: Safar -->

	<!-- launch the comm node -->

	<node pkg="robotiq_vacuum_grippers_control" type="RobotiqVacuumGrippersRtuNode.py" name="RobotiqVacuumGrippersRtuNode" output="screen" respawn="true" args="/dev/ttyTool"/>

	<!-- launch the controller -->

	<node pkg="robotiq_vacuum_grippers_control" output="screen" type="robotiq_vacuum_grippers_ctrl.py" name="robotiq_vacuum_grippers_ctrl" respawn="true" args="/dev/ttyTool"/>

</launch>   